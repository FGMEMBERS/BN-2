<?xml version="1.0"?>

<PropertyList>

 <path>gauge-45mm.ac</path>
 
 <animation>
	 <type>material</type>
	 <object-name>face</object-name>
	 <texture>tach.png</texture>
</animation>

<animation>
  <type>material</type>
  <object-name>face</object-name>
  <emission>
	  <red>0.5</red>
	  <green>0.2</green>
	  <blue>0.0</blue>
	  <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>/engines/engine[0]/rpm</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep> -108  </dep></entry>     
   <entry><ind>  0.1</ind><dep> -107.5</dep></entry>     
   <entry><ind>  500.0</ind><dep>-72  </dep></entry>     
   <entry><ind> 1000.0</ind><dep>-36  </dep></entry>     
   <entry><ind> 1500.0</ind><dep>  0  </dep></entry>     
   <entry><ind> 2000.0</ind><dep> 36  </dep></entry>     
   <entry><ind> 2500.0</ind><dep> 72  </dep></entry>     
   <entry><ind> 3000.0</ind><dep>108  </dep></entry>     
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


