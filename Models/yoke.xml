<?xml version="1.0"?>

<PropertyList>
	
	<path>yoke.ac</path>
	
	<animation>
		<type>translate</type>
		<object-name>yoke-assembly</object-name>
		<property>controls/flight/elevator</property>
		<factor>-0.03</factor>
		<axis>
			<x>1.0</x>
			<y>0.0</y>
			<z>0.0</z>
		</axis>
 	</animation>
 
	<animation>
		<type>rotate</type>
		<object-name>yoke-assembly</object-name>
		<property>controls/flight/aileron</property>
		<factor>-90</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1.0</x>
			<y>0.0</y>
			<z>0.0</z>
		</axis>
	</animation>
	
</PropertyList>
